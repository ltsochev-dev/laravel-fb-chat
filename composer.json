{
    "name": "ltsochev/laravel-fb-chat",
    "description": "A simple Laravel library that wraps the Facebook customer chat plugin in your website.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lachezar Tsochev",
            "email": "info@ltsochev.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ltsochev\\CustomerChat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ltsochev\\CustomerChat\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ltsochev\\CustomerChat\\CustomerChatServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^3.1"
    }
}
